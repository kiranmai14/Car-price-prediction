  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "#removing New_Price column as it contains more null values\n",
    "data = df.drop(columns = ['New_Price'] )\n",
    "test = test.drop(columns = ['New_Price'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "#removing null values for Mileage by filling with mean\n",
    "data['Mileage']=data['Mileage'].str.split(\" \").str[0].astype('float64')#splitting the value and units and accessing only values\n",
    "data['Mileage']=data['Mileage'].fillna(np.mean(data['Mileage']))\n",
    "\n",
    "test['Mileage']=test['Mileage'].str.split(\" \").str[0].astype('float64')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "#removing null values for Engine by filling with mean\n",
    "data['Engine']=data['Engine'].str.split(\" \").str[0].astype('float64') #splitting the value and units and accessing only values\n",
    "data['Engine']=data['Engine'].fillna(np.mean(data['Engine']))\n",
    "\n",
    "test['Engine']=test['Engine'].str.split(\" \").str[0].astype('float64') #splitting the value and units and accessing only values\n",
    "test['Engine']=test['Engine'].fillna(np.mean(test['Engine']))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "#removing null values for Seats by filling with mode\n",
    "from scipy import stats\n",
    "c= stats.mode(data['Seats'])\n",
    "data['Seats'] = data['Seats'].fillna(c.mode[0])\n",
    "data.loc[data.Seats == 0,'Seats'] = c.mode[0]          #replacing zero seats with mode \n",
    "\n",
    "c= stats.mode(test['Seats'])\n",
    "test['Seats'] = test['Seats'].fillna(c.mode[0])\n",
    "test.loc[test.Seats == 0,'Seats'] = c.mode[0]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "#removing NaN values for Power by filling with mean\n",
    "data['Power']=data['Power'].str.split(\" \").str[0]\n",
    "data.loc[data.Power == 'null','Power'] = '0'       #replacing 'null' string with '0'\n",
    "data['Power']=data['Power'].astype('float64')      #converting to float\n",
    "x=np.mean(data['Power'])\n",
    "data['Power']=data['Power'].fillna(x)              #filling with mean\n",
    "data.loc[data.Power == 0.00,'Power'] = x           #fillig 0.00 values with mean\n",
    "\n",
    "test['Power']=test['Power'].str.split(\" \").str[0]\n",
    "test.loc[test.Power == 'null','Power'] = '0'       #replacing 'null' string with '0'\n",
    "test['Power']=test['Power'].astype('float64')      #converting to float\n",
    "x=np.mean(test['Power'])\n",
    "test['Power']=test['Power'].fillna(x)              #filling with mean\n",
    "test.loc[test.Power == 0.00,'Power'] = x  "
   ]
